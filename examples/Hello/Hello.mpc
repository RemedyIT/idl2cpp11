// -*- MPC -*-
// $Id$

project(*hello_Idl): ridl_defaults {
  IDL_Files {
    Test.idl
  }
  custom_only = 1
}

project(*hello_Server): taox11_server {
  after += *hello_Idl
  Source_Files {
    Hello.cpp
    server.cpp
  }
  Source_Files {
    TestC.cpp
    TestS.cpp
  }
  IDL_Files {
  }
}

project(*hello_Client): taox11_client {
  after += *hello_Idl
  Source_Files {
    client.cpp
  }
  Source_Files {
    TestC.cpp
  }
  IDL_Files {
  }
}

